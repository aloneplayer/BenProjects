<UserControl x:Class="BabyGames.Controls.Snake.SnakeSkin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="16" Height="16">
    <Grid x:Name="LayoutRoot">
        <Canvas>
            <Ellipse Width="16" Height="16">
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.4,0.4" Center="0.4,0.4" RadiusX="0.7" RadiusY="0.7">
                        <GradientStop Color="#F5F2F6" Offset="0.0" />
                        <GradientStop Color="#514545" Offset="0.75" />
                        <GradientStop Color="#302C2C" Offset="1" />
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
        </Canvas>
    </Grid>
</UserControl>
