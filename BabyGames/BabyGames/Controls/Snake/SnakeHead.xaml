<custom:SnakeBody x:Class="BabyGames.Controls.Snake.SnakeHead"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:custom="clr-namespace:BabyGames.Controls"
    Width="16" Height="16">
    <Grid x:Name="LayoutRoot">
        <Canvas x:Name="canvasHead" Width="16" Height="16" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False">
            <Ellipse Width="16" Height="16" Fill="#FFC6615A" Stroke="#7FB6BD" StrokeThickness="1" />
            <Ellipse Width="6" Height="6" Fill="#F5F2F6" Canvas.Top="3" Canvas.Left="2" Opacity="0.7" />
            <Ellipse Width="6" Height="6" Fill="#F5F2F6" Canvas.Top="3" Canvas.Left="8" Opacity="0.7" />
            <Ellipse Width="3" Height="3" Fill="#161A1B" Canvas.Top="4" Canvas.Left="3.5" />
            <Ellipse Width="3" Height="3" Fill="#161A1B" Canvas.Top="4" Canvas.Left="9.5" />
            <Canvas.RenderTransform>
                <TransformGroup>
                    <RotateTransform x:Name="rtHead" Angle="0" />
                </TransformGroup>
            </Canvas.RenderTransform>
        </Canvas>
    </Grid>
</custom:SnakeBody>
