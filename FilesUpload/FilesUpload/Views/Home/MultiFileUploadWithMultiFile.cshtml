@{
    ViewBag.Title = "JQueryMultiFileUpload";
}
<h2>
    JQueryMultiFileUpload</h2>
<link href="@Url.Content("~/Content/upload.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/ThirdPartyLib/jquery-1.7.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/ThirdPartyLib/jquery.MultiFile.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/MultiFileUploadWithMultiFile.js")" type="text/javascript"></script>
@using (Html.BeginForm("MultiFileUploadWithMultiFile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <span class="btn btn-success fileinput-button"><i class="icon-plus icon-white"></i><span>
        Add files...</span>
        <input type="file" name="files" id="myfileupload">
    </span>
    <input type="submit" />
    <div id="fileList" />
}
